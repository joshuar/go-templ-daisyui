package components

func (c *modifierBaseColor) setBaseColor() string {
	switch c.baseColor {
	case ColorBgBase100:
		return "bg-base-100"
	case ColorBgBase200:
		return "bg-base-200"
	case ColorBgBase300:
		return "bg-base-300"
	default:
		return "bg-base-100"
	}
}

// noop function so that templ generation doesn't report errors.
templ colorNOOP() {
}
